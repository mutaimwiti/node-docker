{
  "name": "node-docker",
  "version": "1.0.0",
  "description": "Node Docker",
  "main": "src/index.js",
  "author": "Mutai Mwiti",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && babel ./src -d dist --copy-files",
    "start": "yarn build && node dist/index.js",
    "start:dev": "nodemon --exec babel-node src/index.js"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "nodemon": "^1.19.3"
  }
}
